.unlock-status-container {
  background-color: white;
  border-radius: 16rpx;
  padding: 20rpx 30rpx;
  margin: 20rpx 0;
  box-shadow: 0 2rpx 10rpx rgba(0, 0, 0, 0.05);
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.unlock-status-text {
  font-size: 28rpx;
  color: #333;
  font-weight: 500;
}

.unlock-btn {
  font-size: 24rpx;
  color: white;
  background-color: #6E7BF2;
  padding: 8rpx 20rpx;
  border-radius: 30rpx;
} 